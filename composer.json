{
  "name": "pollen-solutions/wp-cleaner",
  "description": "Wordpress Cleaner Component",
  "homepage": "https://github.com/milkcreation/pollen-wp-cleaner.git",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "cleaner"
  ],
  "time": "2021-09-16",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "pollen-solutions/http": "^1.0",
    "pollen-solutions/support": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpCleaner\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpCleaner\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
